{
  "name": "node-typescript-eslint-prettier-vitest",
  "version": "1.0.0",
  "description": "Example ESM starter project using Node, TypeScript, eslint, prettier and vitest",
  "type": "module",
  "main": "./build/main.js",
  "exports": "./build/main.js",
  "types": "./build/main.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx ./src/main.ts",
    "format": "prettier . --write",
    "lint": "eslint .",
    "test": "vitest run ./tests"
  },
  "author": "kmturley",
  "license": "MIT",
  "keywords": [
    "node",
    "typescript",
    "eslint",
    "prettier",
    "vitest"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "eslint": "^9.12.0",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "tsx": "^4.10.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vitest": "^1.6.0"
  }
}
